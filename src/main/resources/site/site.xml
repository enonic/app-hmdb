<?xml version="1.0" encoding="UTF-8"?>
<site xmlns="urn:enonic:xp:model:1.0">
    <x-data name="SoMe" allowContentTypes="person|movie"/>
    <form/>
    <mappings>
        <mapping controller="/controllers/graphql.js" order="50">
            <pattern>/api</pattern>
        </mapping>
        <mapping controller="/lib/external-frontend/proxy.js" order="99">
            <!--FIXME: Matching only the pattern PROXY_MATCH_PATTERN (see proxy.es6) and existing contents, and returning
             404 for the rest should be handled here. But until https://github.com/enonic/xp/issues/8530 is fixed,
             this is handled manually in proxy.es6. -->
            <match>type:'.+:.+'</match>
        </mapping>
    </mappings>
</site>
